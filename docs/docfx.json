{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "SharpHook/bin/Release/net8.0/SharpHook.dll",
            "SharpHook.Reactive/bin/Release/net8.0/SharpHook.Reactive.dll"
          ],
          "src": "../"
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net8.0"
      },
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "logo.png",
          "favicon.ico"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "SharpHook",
      "_appName": "SharpHook",
      "_appFooter": "<span>Created by <a href=\"https://tolik.io\">Tolik Pylypchuk</a>. Docs made with <a href=\"https://dotnet.github.io/docfx\">docfx</a></span>.",
      "_appLogoPath": "logo.png",
      "_appFaviconPath": "favicon.ico",
      "_enableSearch": "true",
      "_lang": "en"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "template"
    ],
    "sitemap": {
      "baseUrl": "https://sharphook.tolik.io/v5.3.5/"
    },
    "postProcessors": [ "ExtractSearchIndex" ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
